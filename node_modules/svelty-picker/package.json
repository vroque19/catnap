{
  "name": "svelty-picker",
  "description": "Sweet date/time picker written in svelte",
  "version": "5.2.9",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "ce": "vite build --mode ce",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "generate": "node scripts/generate_examples.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./i18n": {
      "types": "./dist/i18n/index.d.ts",
      "default": "./dist/i18n/index.js"
    },
    "./element": {
      "types": "./dist/utils/custom-element.d.ts",
      "default": "./dist/utils/custom-element.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^3.53.0 || ^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@bitmachina/highlighter": "1.0.0-alpha.5",
    "@jridgewell/sourcemap-codec": "https://github.com/xiaoxiangmoe/sourcemap-codec/tarball/main",
    "@prgm/sveltekit-progress-bar": "^1.0.1",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^2.5.7",
    "@sveltejs/package": "^2.2.6",
    "@sveltejs/repl": "^0.5.0",
    "@sveltejs/site-kit": "^5.2.2",
    "@sveltejs/vite-plugin-svelte": "^2.5.3",
    "autoprefixer": "^10.4.17",
    "mdsvex": "^0.11.0",
    "postcss": "^8.4.35",
    "postcss-load-config": "^4.0.2",
    "publint": "^0.1.16",
    "shiki": "^0.14.7",
    "svelte": "5.0.0-next.125",
    "svelte-check": "^3.6.4",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^4.5.2",
    "vite-plugin-node-polyfills": "^0.9.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.4"
  }
}
